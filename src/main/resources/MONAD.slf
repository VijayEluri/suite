using STANDARD >>
define m-put = (
) >>
define m-seq = (
	define seq as (:t :- any => :t => :t) = (
		CLASS!suite.lp.invocable.Invocables$Seq | ijavacls | ijavaobj2
	) >>
	do0 => do1 => (do #
		seq {exec {do0}} {exec {do1}}
	)
) >>
define m-seqs = (
	dos => fold-right {m-seq} {do # Nothing} {dos}
) >>
define m-while = (
	cond => loop => (do #
		if:: exec {cond}
		then:: exec {m-seq {loop} {m-while {cond} {loop}}}
		else:: Nothing
	)
) >>

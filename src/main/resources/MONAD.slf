using STANDARD >>
define seqm = (
	define seq as (:t :- any => :t => :t) = (
		CLASS!suite.lp.invocable.Invocables$Seq | ijavacls | ijavaobj2
	) >>
	do0 => do1 => (do #
		seq {exec {do0}} {exec {do1}}
	)
) >>
define seqsm = (
	dos => fold-right {seqm} {do # Nothing} {dos}
) >>
define whilem = (
	cond => loop => (do #
		if:: exec {cond}
		then:: exec {seqm {loop} {whilem {cond} {loop}}}
		else:: Nothing
	)
) >>
